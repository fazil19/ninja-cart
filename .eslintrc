{
  "parser": "babel-eslint",
  "rules": {
    "linebreak-style": 0,
    "semi": [2, "always"],
    "no-console": 0
  },
  "env": {
    "es6": true,
    "browser": true,
    "jasmine": true
  },
  "globals": {
    "inject": true,
    "module": true
  },
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "modules" : true
  }
}